CALL WSETCONTENTTYPE("application/json")

CALL WSETHEADER("Access-Control-Allow-Origin","*")

* Get our request_method 

CALL WGETHEADER(REQUEST.METHOD,"REQUEST_METHOD")

CALL WOBJ(RESPONSE.OBJ,"FROMSTRING","","{}","",RERR)

EXECUTE "TSORT NW.ORDERS ORDERID CUSTOMERID ORDERDATE *A6 PRODUCTID PRODUCTDESCRIPTION UNITPRICE QUANTITY" CAPTURING JSON

CALL WOBJ(RESPONSE.OBJ,"SET.ARRAY","result",JSON,"",RERR)

CALL WOBJ(RESPONSE.OBJ,"TOSTRING","",SEND.JSON,"PRETTIFY",RERR)

CALL WSEND(SEND.JSON)

